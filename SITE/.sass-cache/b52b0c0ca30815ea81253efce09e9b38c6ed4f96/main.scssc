o:Sass::Tree::RootNode
:@template""@import "constants";

body {
  	font-family: 'Maven Pro';
	position: relative;
	overflow-x: hidden;
}

ul, li {
	list-style-type : none;
}

h1 {
	margin: 0.5em 0;
}

h2 {
	margin: 0.5em 0;
	font-weight: normal;
	font-size: 1.2em;
}

a{
	color: #000;
	text-decoration: none;
	&:hover {
		text-decoration: underline;
	}
}

.wrapper {
	width: 1005px;
	margin: 0 auto;
	position: relative;
}

.wrapper-small {
	width: 950px;
	margin: 0 auto;
}

.gray-bg {
	background: $gray-color;
}

.header-container {
	.title {
		background: url('../img/dcg-logo.jpg') 0 0 no-repeat;
		width: 102px;
		height: 102px;
		float: left;
	}
	.slogan {
		font-size: 1.3em;
		margin: 1.9em 4% 0 0%;
		width: 710px;
		float: right;
		color: $gray-color;
	}
}

.footer-container {
	background: $gray-color;
	height: 30px;
	footer {
		color: white;
		font-weight: normal;
	}
	h5 {
		padding: 5px;
	}
}

/* ==============
	MOBILE: Menu
   ============== */
nav ul {
	width: 55%;
	float: right;
	margin: 0;
	padding: 0;
	li {
		float: left;
		margin: 0 1.7%;
		padding: 25px 2%;
		margin-bottom: 0;
		&.last {
			margin: 0 0 0 1.7%;
			padding: 25px 0 25px 2%;
		}

	}
	a {
		display: block;

		text-align: center;
		font-weight: normal;
		font-size: 15px;

		color: $orange-color;
		background: #fff;
		border-bottom: 1px solid #fff;

		&:hover,
		&:visited {
			color: $orange-color;
		}

		&:hover {
		}
	}
}


/* ==============
	MOBILE: Main
   ============== */

.main-container {
	width:100%;
}
.main {
	padding: 30px 0;
}

.main article h1 {
	font-size: 2em;
}

header h1 {
	color: $orange-color;
}


/* ==================
	SECTIONS
	================== */

.overlay-container {
	position: absolute;
	z-index: 1000;
	width: 100%;
	height: 100%;
	display: none;
	margin-top:-180px;
}

.overlay {
	width: 100%;
	height: 100%;
	display: none;
	position: relative;

	.bg {
		background: #000;
		opacity: .8;
		width: 100%;
		height: 100%;
		position: relative;
		z-index: 0;
		zoom: 1;
		display: block;
	}
	.content {
		>.inner {
			padding: 20px;
		}
		h3 {
			color: $orange-color;
			font-size: 25px;
			margin: 0;
		}
		h4 {
			font-size: 20px;
			margin: 0;
		}
		p {
			font-size: 14px;
		}
		.email, .info, .twitter {
			color: $orange-color;
			margin: 0;
			span{
				margin: 0px 10px 0px 0px;
			}
			a {
				color: $orange-color;
			}
		}
		background: #fff;
		width: 75%;
		text-align: left;
		position: fixed;
		z-index: 1;
		zoom: 1;
		top: 10%;
		left: 50%;
		margin-left: -37.5%;
	}
	.close {
		float: right;
		background: url('../img/close-overlay.jpg') 0 0 no-repeat;
		width: 19px;
		height: 17px;
		display: block;
	}
	hr{
		margin-top: 30px;
		border: 1px dotted $gray-color;
	}
}

#steps {
	color : #fff;
	section {
		float: left;
		width: 235px;
		height: 100%;
		margin:0px 2% 0px 0px;
		text-align: center;
		overflow: hidden;

		h2 {
			color: $blue;
			line-height: 1em;
			font-size: 1.2em;
		}
		p {
			color: $light-gray;
			text-align: justify;
		}
		> .inner {
			padding: 10px;
		}
		&.last {
			margin: 0;
		}
	}
	article {
		padding: 20px 0px 0px 0px;
	}
}

#definition {
	text-align: center;

	article{
		padding: 35px 0px 25px 0px;
		header {
			h1 {
				font-size: 2em;
				padding: 0px;
				margin: 0px;
			}
		}
		p {
			font-size: 1em;
		}
	}
}

#latest {
	background: $light-tan;
	border-top: 5px solid $orange-color;
	article {
		padding: 0 0 2% 0;
	}
	.wrapper {
		> .inner {
			padding: 1% 2%;
		}
	}
	section{
		overflow: hidden;
		position: relative;
		float: left;
		width: 24.6%;
		border-right: 1px dotted $gray-color;
		min-height: 310px;
		height: 100%;

		> .inner {
			padding: 1% 5%;
		}
		&.last{
			border-right: 0;
			border-bottom: 0;
		}
		p {
			margin: 0;
		}
		h2{
			font-weight: bold;
		}
		.date {
			font-size: 14px;
			font-weight: bold;
			color: $gray-color;
		}
		.title {
			font-size: 16px;
			color: $gray-color;
		}
		.seeall {
			position: absolute;
			bottom: 0px;
			right: 10px;
		}
		li {
			margin-top: 10px;
		}
	}
}

#thoughts {
	.contain {
		border-bottom: 1px dotted $gray-color;
	}
	.row {
		border-top: 1px dotted $gray-color;
		margin: 5px 0 0 0;
		width: 100%;
		position: inherit;
		>.inner {
			position: relative;
			padding: 8px 5px 5px 5px;
			width: 100%;
		}
	}
	.wrapper {
		>.inner {
			padding: 1% 2%;
		}
	}
	section {
		position: relative;
		float: left;
		width: 24.6%;
		min-height: 310px;
		height:100%;
		border-right: 1px dotted $gray-color;
		&.first{
		}
		&.last{
			border-right: 0;
		}
		>.inner {
			padding: 0px 8px;
		}
		.blog-btn {
			display: block;
			min-height: 310px;
			height:100%;
		}
		.share {
			margin-top: 1em;
			iframe {
				margin: -3px 0 0 0;
			}
		}
		p {
			margin: 0;
			&.date{

				font-size: 11px;
				margin-bottom: 5px;
				color: #999;
			}
			&.title{
				font-size: 16px;
				line-height: 18px;
				height: 55px;
				overflow: hidden;
			}
			&.continue{
				font-size: 14px;
			}
			&.author{
				font-size: 11px;
				line-height: 11px;
			}
		}
		img {
			max-width: 100%;
			margin: .5em 0;
			overflow: hidden;
		}
	}
}

#social {
	article {
		padding: 0 0 2em 0;
		>.inner {
			padding: 1% 2%;
		}
	}
	#videoplayer{
		width:48.4%;
		border-right: 1px dotted $gray-color;
		padding: 0 .9em 0 0px;
		>.inner {
			padding: 0px .3em 1em .3em;
			background: #eaeaea;
		}
		#videoLoad{
			display: none;
			visibility: hidden;
		}
		#video {
			padding: 2em 1em 0 0;
		}
		#videofeature {
			h3 {
				max-height: 55px;
				overflow: hidden;
				border-top: 1px dotted black;
				padding: 2% 0px;
			}
			iframe{
				width: 100%;
			}
			.contain-seeall {
				.seeall {
					padding: 10px 0;
					display: block;
					float:right;
					margin-right: 10px;
				}
			}
		}
		#videoplaylist {
			h4 {
				width: 90px;
				height: 57px;
				margin: 0;
				font-size: 13px;
				overflow: hidden;
			}
			img {
				width: 90px;
			}
			li {
				display: inline;
				float: left;
				margin: 0px 0px 0px 2px;
				&.active {
					//background: #f1f1f1;
					opacity: .3;
					cursor: cursor;
					a {
						cursor: cursor;
						text-decoration: none;
					}
				}
			}
		}
	}
	#twitter {
		width: 49%;
		>.inner {
			padding: 10px 20px;
		}
		.tweet_list {
			margin: 0;
			li {
				border-bottom: 1px dotted $gray-color;
				padding: 0px 0px 5px 0px;
				margin: 10px 0px;
				overflow: hidden;

				.tweet_time{
					display: block;
				}
			}
			.tweet_first {
				margin: 0px 0px 10px 0px;
			}
			.tweet_last {
				border-bottom: 0;
			}
			.tweet_avatar {
				border: 1px solid #000;
				padding: 3px;
				width: 64px;
				height: 64px;
				display: block;
				float: left;
				margin-right: .75em;
			}
			.txt {
				margin: 0;
				width: 75%;
				float: left;
			}
		}
		header {
			border-bottom: 1px dotted $gray-color;
			margin-bottom: 1em;
			h2 {
				float: left;
				margin: 0;
				margin-bottom: .5em;
			}
			a {
				margin-top: .5em;
				float: right;
				display: block;
				margin: 5px 10px;
			}
		}
	}
	section {
		float: left;
	}
}

#story {
	color: #fff;

	.wrapper {
		> .inner {
			padding: 1% 2%;
		}
		section{
			p{
				font-size: 1.2em;
			}
		}
	}
}

#team {
	background: #000;
	color: #fff;
	position: relative;
	border-top: 5px solid $orange-color;
	border-bottom: 6px solid $orange-color;
	section {
		border-right: 1px dotted #fff;
		float: left;
		margin: 0px 1px 0px 0px;
		text-align: center;
		width: 24%;

		a {
			display: block;
			color: #fff;
			position: relative;
			img {
				background: #010101;
				width: 100%;
			}
			.info {
				padding: 15px 0px;
				p{
					margin: 0;
				}
			}
		}

		&.last {
			border-right: 0;
		}
	}
	.row {
		border-bottom: 1px dotted #fff;
		&.last {
			border-bottom: 0;
		}
	}
	article {
		z-index: 1;
		zoom: 1;
	}
}

#contact {
	.wrapper {
		> .inner {
			padding: 1% 2%;
		}
	}
	section {
		float: left;
		border-right: 1px dotted $gray-color;
		min-height: 275px;
		height: 100%;

		&.last {
			border-right: 0;
		}



		h4{
			margin: 0;
			font-size: 18px;
		}
		p {
			margin: 0;
			font-size: 14px;
			line-height: 1.5em;
		}
	}
	.links {
		width: 24%;
		> .inner {
			padding: 0 0 0 1em;
		}
		img {
			margin-top: -7px;
		}
		span{
			margin: 5px;
		}
	}
	.newsletter {
		width: 48%;
		> .inner {
			padding: 0 1em;
		}
		input {
			border: 1px solid $gray-color;
			padding: 15px;
			background: rgba(255,255,255,1);
			margin: 0 0 10px 0;
		}
		input[type='text'] {
			width: 65%;
		}
		input[type='submit'] {
			background: #638d95;
  			font-family: 'Maven Pro';
			&:hover{
				background: $orange-color;
			}
		}
		p{
			margin: 0 0 20px 0;
		}
	}
	.contact {
		width: 24%;
		> .inner {
			padding: 0 0 0 1em;
		}
		.role{
			margin-bottom: 15px;
		}
		.email, .phone {
			overflow: hidden;
		}
	}
